# 1.3.0
# First line is the parameter file version. Don't change it.
thread_num = 0
percolator_path = C:\Program Files\percolator-v3-01\bin\percolator.exe # Please input absolute path of Percolator binary file

# Database
db = test.fasta
missed_cleavage = 2 # maximum number of missed cleavage
min_peptide_length = 7
max_peptide_length = 50

# Spectrum
min_ms1_charge = 1
max_ms1_charge = 7
max_ms2_charge = 3 # min ms2 charge is set to 1
min_peak_num = 10
min_potential_charge = 2 # the min precursor charge tried if there is no charge information for a MS/MS spectrum
max_potential_charge = 4 # the max precursor charge tried if there is no charge information for a MS/MS spectrum
ms_level = 2,3 # the searched MS level

# Tolerance
ms1_tolerance_unit = 1 # 0: Da; 1: ppm
ms1_tolerance = 10
ms2_tolerance = 0.01
mz_bin_offset = 0
min_clear_mz = 112.5 # clear out all peaks in the specified m/z range, mainly for iTRAQ/TMT data
max_clear_mz = 121.5

# Modification related
min_ptm_mass = -250
max_ptm_mass = 250

# considered modifications in generating tags
# specify additional mass and amino acid. DO NOT change the last character.
# maximum number is 10
# empty entries must start with 0.0
mod01 = 79.966331@S~ # Phosphorylation
mod02 = 79.966331@T! # Phosphorylation
mod03 = 79.966331@Y% # Phosphorylation
mod04 = 42.010565@K^ # Acetylation
mod05 = 15.994915@M& # Oxidation
mod06 = 14.015650@K* # Methylation
mod07 = 0.0@X+
mod08 = 0.0@X<
mod09 = 0.0@X>
mod10 = 0.0@X?

# considered peptide/protein N/C-terminal modifications in generating tags
# empty ones must with value 0.0
# the maximum number for each terminal is 9
pepNterm = 0.0
pepCterm = 0.0
proNterm = 42.010565 # Acetylation
proCterm = 0.0

# Fix modification
G = 0
A = 0
S = 0
P = 0
V = 0
T = 0
C = 57.02146
I = 0
L = 0
N = 0
D = 0
Q = 0
K = 0
E = 0
M = 0
H = 0
F = 0
R = 0
Y = 0
W = 0
O = 0 # Pyrrolysine
U = 0 # Selenocysteine
n = 0 # nterm mod
c = 0 # cterm mod

# Enzyme digestion specificities
# enzyme name  	 is cut from C-term 	 cleavage site 	 protection site
Trypsin        	 1                  	 KR            	 P
# TrypsinR     	 1                  	 R             	 P
# LysC         	 1                  	 K             	 -
# Chymotrypsin 	 1                  	 FYW           	 P
# GluC         	 1                  	 DE            	 -
# LysN         	 0                  	 K             	 -
# AspN         	 0                  	 DE            	 -

# Do not change the following
output_percolator_input = 1
